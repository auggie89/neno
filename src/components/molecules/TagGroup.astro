---
import Tag from "../atoms/Tag.astro";
const titles = ["CSS", "HTML", "Middleman", "Ruby", "HAML", "SASS", "Git"] 
---

<ul class="gm-tag-group">
  {titles.map(title => (
    <Tag title={title} />
  ))}
</ul>

<style>
  .gm-tag-group {
    display: flex;
    justify-content: space-between;
    margin-top: 1.25rem;
  }
</style>
